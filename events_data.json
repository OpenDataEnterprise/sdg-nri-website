---
layout: null
---
[
  {% for event in site.events %}{
    "title": "{{ event.title }}",
    "date": "{{ event.date }}",
    "start_date": "{{ event.start_date }}",
    "end_date": "{{ event.end_date }}",
    "locations": [{% for location in event.locations %}
      "{{ location }}"{% unless forloop.last %},{% endunless %}{% endfor %}
    ],
    "tags": [{% for tag in news.tags %}
      "{{ tag }}"{% unless forloop.last %},{% endunless %}{% endfor %}
    ]
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
