---
layout: null
---
[
  {% for news in site.news %}{
    "title": "{{ news.title }}",
    "author": "{{ news.author }}",
    "date": "{{ news.date }}",
    "url": "{{ news.link }}",
    "description": "{{ news.description }}",
    "tags": [{% for tag in news.tags %}
      "{{ tag }}"{% unless forloop.last %},{% endunless %}{% endfor %}
    ]
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
