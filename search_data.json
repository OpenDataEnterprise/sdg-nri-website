---
layout: null
---
[
  {% for resource in site.resources %}{
    "title": "{{ resource.title }}",
    "organization": "{{ resource.organization }}",
    "date_published": "{{ resource.date_published }}",
    "url": "{{ resource.link }}",
    "type": "{{ resource.type }}",
    "description": "{{ resource.description }}",
    "languages": [{% for language in resource.languages %}
      "{{ language }}"{% unless forloop.last %},{% endunless %}{% endfor %}
    ],
    "tags": [{% for tag in resource.tags %}
      "{{ tag }}"{% unless forloop.last %},{% endunless %}{% endfor %}
    ]
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
