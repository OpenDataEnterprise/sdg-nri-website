{
  "name": "sdg-prototype",
  "version": "1.0.0",
  "description": "*Minima is a one-size-fits-all Jekyll theme for writers*. It's Jekyll's default (and first) theme. It's what you get when you run `jekyll new`.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:prod": "NODE_ENV=production webpack -p --config webpack.config.js",
    "build:staging": "NODE_ENV=staging webpack -p --config webpack.config.js; jekyll build --config _config.yml,_config-staging.yml",
    "build:dev": "NODE_ENV=development webpack -p --config webpack.config.js; jekyll build --config _config.yml,_config-development.yml"
  },
